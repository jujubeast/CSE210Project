<h1>Welcome to the Website!</h1>

<h1>Sign in</h1>

<%=form_for :session, :url => sessions_path do |f| %>

	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :password %>
	<%= f.password_field :password %>

	<%= f.submit "Sign in" %>
<% end %>

<p>Or...<%= link_to "Sign up!", signup_path %></p>
